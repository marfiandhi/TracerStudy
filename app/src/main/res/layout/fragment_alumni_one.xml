<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorDarkBackground"
    tools:context=".ui.quiz.fragment.AlumniOneFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorDarkForeground"
            android:gravity="center"
            android:orientation="horizontal"
            android:padding="4dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Pertanyaan dengan tanda"
                android:textColor="@color/colorWhite"
                android:textSize="16sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=" * "
                android:textColor="@color/colorLightRed"
                android:textSize="16sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="wajib diisi."
                android:textColor="@color/colorWhite"
                android:textSize="16sp" />

        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginTop="4dp"
            android:layout_marginBottom="4dp"
            android:padding="4dp"
            android:text="Identitas Diri"
            android:textAllCaps="true"
            android:textColor="@color/colorLightGrey" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorDarkForeground"
            android:orientation="vertical"
            android:padding="4dp">

            <android.support.design.widget.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:textColor="@color/colorWhite"
                android:textColorHint="@color/colorGrey">

                <EditText
                    android:id="@+id/alumni_one_et_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Nama lengkap*"
                    android:inputType="textPersonName|textCapWords"
                    android:lines="1"
                    android:maxLength="32"
                    android:selectAllOnFocus="true"
                    android:textColor="@color/colorWhite" />

            </android.support.design.widget.TextInputLayout>

            <TextView
                android:id="@+id/alumni_one_tv_gender"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="3dp"
                android:layout_marginTop="4dp"
                android:text="Jenis kelamin*"
                android:textColor="@color/colorGrey"
                android:textSize="12sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="4dp"
                android:gravity="center"
                android:orientation="vertical">

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/alumni_one_et_gender_layout"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:textColor="@color/colorWhite"
                    android:textColorHint="@color/colorGrey"
                    android:visibility="gone">

                    <EditText
                        android:id="@+id/alumni_one_et_gender"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Jenis kelamin*"
                        android:inputType="text"
                        android:lines="1"
                        android:maxLength="9"
                        android:selectAllOnFocus="true"
                        android:textColor="@color/colorWhite" />

                </android.support.design.widget.TextInputLayout>

                <RadioGroup
                    android:id="@+id/alumni_one_radio_group"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <RadioButton
                        android:id="@+id/alumni_one_radio_male"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:checked="true"
                        android:text="Laki-laki"
                        android:textColor="@color/colorWhite"
                        app:buttonTint="@color/colorLightGrey" />

                    <RadioButton
                        android:id="@+id/alumni_one_radio_female"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Perempuan"
                        android:textColor="@color/colorWhite"
                        app:buttonTint="@color/colorLightGrey" />

                </RadioGroup>

            </LinearLayout>

        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginTop="4dp"
            android:layout_marginBottom="4dp"
            android:padding="4dp"
            android:text="Pendidikan sebelumnya"
            android:textAllCaps="true"
            android:textColor="@color/colorLightGrey" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorDarkForeground"
            android:orientation="vertical"
            android:padding="4dp">

            <android.support.design.widget.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:textColor="@color/colorWhite"
                android:textColorHint="@color/colorGrey">

                <EditText
                    android:id="@+id/alumni_one_et_school_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Asal SMA/SMU/SMP (Jika dari SMP)"
                    android:inputType="textCapWords|textMultiLine"
                    android:maxLines="3"
                    android:selectAllOnFocus="true"
                    android:textColor="@color/colorWhite" />

            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:orientation="vertical"
                android:textColor="@color/colorWhite"
                android:textColorHint="@color/colorGrey">

                <EditText
                    android:id="@+id/alumni_one_et_school_province"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Provinsi"
                    android:inputType="textCapWords"
                    android:maxLength="24"
                    android:maxLines="1"
                    android:selectAllOnFocus="true"
                    android:textColor="@color/colorWhite"
                    app:autoSizeTextType="uniform" />

            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:orientation="vertical"
                android:textColor="@color/colorWhite"
                android:textColorHint="@color/colorGrey">

                <EditText
                    android:id="@+id/alumni_one_et_school_city"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Kota"
                    android:inputType="textCapWords"
                    android:maxLength="20"
                    android:maxLines="1"
                    android:selectAllOnFocus="true"
                    android:textColor="@color/colorWhite"
                    app:autoSizeTextType="uniform" />

            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:orientation="vertical"
                android:textColor="@color/colorWhite"
                android:textColorHint="@color/colorGrey">

                <EditText
                    android:id="@+id/alumni_one_et_school_postcode"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Kode Pos"
                    android:inputType="number"
                    android:maxLength="7"
                    android:maxLines="1"
                    android:selectAllOnFocus="true"
                    android:textColor="@color/colorWhite"
                    app:autoSizeTextType="uniform" />

            </android.support.design.widget.TextInputLayout>

        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginTop="4dp"
            android:layout_marginBottom="4dp"
            android:padding="4dp"
            android:text="Tempat/tanggal lahir"
            android:textAllCaps="true"
            android:textColor="@color/colorLightGrey" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorDarkForeground"
            android:orientation="vertical"
            android:padding="4dp">

            <android.support.design.widget.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:textColor="@color/colorWhite"
                android:textColorHint="@color/colorGrey">

                <EditText
                    android:id="@+id/alumni_one_et_born_province"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Provinsi*"
                    android:inputType="textCapWords"
                    android:maxLength="24"
                    android:maxLines="1"
                    android:selectAllOnFocus="true"
                    android:textColor="@color/colorWhite"
                    app:autoSizeTextType="uniform" />

            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:orientation="vertical"
                android:textColor="@color/colorWhite"
                android:textColorHint="@color/colorGrey">

                <EditText
                    android:id="@+id/alumni_one_et_born_city"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Kota*"
                    android:inputType="textCapWords"
                    android:maxLength="20"
                    android:maxLines="1"
                    android:selectAllOnFocus="true"
                    android:textColor="@color/colorWhite"
                    app:autoSizeTextType="uniform" />

            </android.support.design.widget.TextInputLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp">

                <android.support.design.widget.TextInputLayout
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clickable="false"
                    android:focusable="false"
                    android:orientation="vertical"
                    android:textColor="@color/colorWhite"
                    android:textColorHint="@color/colorGrey">

                    <EditText
                        android:id="@+id/alumni_one_et_born_date"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:clickable="false"
                        android:focusable="false"
                        android:hint="Tanggal lahir*"
                        android:inputType="date"
                        android:maxLength="20"
                        android:maxLines="1"
                        android:selectAllOnFocus="true"
                        android:textColor="@color/colorWhite"
                        app:autoSizeTextType="uniform" />

                </android.support.design.widget.TextInputLayout>

                <Button
                    android:id="@+id/alumni_one_button_date_picker"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/colorTransparent"
                    android:textColor="@color/colorWhite" />

            </FrameLayout>

        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginTop="4dp"
            android:layout_marginBottom="4dp"
            android:padding="4dp"
            android:text="Kantor"
            android:textAllCaps="true"
            android:textColor="@color/colorLightGrey" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorDarkForeground"
            android:orientation="vertical"
            android:padding="4dp">

            <android.support.design.widget.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:textColor="@color/colorWhite"
                android:textColorHint="@color/colorGrey">

                <EditText
                    android:id="@+id/alumni_one_et_office_address"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Alamat kantor"
                    android:inputType="textCapWords|textMultiLine"
                    android:maxLines="3"
                    android:selectAllOnFocus="true"
                    android:textColor="@color/colorWhite" />

            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:orientation="vertical"
                android:textColor="@color/colorWhite"
                android:textColorHint="@color/colorGrey">

                <EditText
                    android:id="@+id/alumni_one_et_office_phone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Nomor telp. kantor"
                    android:inputType="phone"
                    android:maxLength="13"
                    android:maxLines="1"
                    android:selectAllOnFocus="true"
                    android:textColor="@color/colorWhite"
                    app:autoSizeTextType="uniform" />

            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:orientation="vertical"
                android:textColor="@color/colorWhite"
                android:textColorHint="@color/colorGrey">

                <EditText
                    android:id="@+id/alumni_one_et_office_postcode"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Kode Pos"
                    android:inputType="number"
                    android:maxLength="7"
                    android:maxLines="1"
                    android:selectAllOnFocus="true"
                    android:textColor="@color/colorWhite"
                    app:autoSizeTextType="uniform" />

            </android.support.design.widget.TextInputLayout>

        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginTop="4dp"
            android:layout_marginBottom="4dp"
            android:padding="4dp"
            android:text="Alamat dan Kontak"
            android:textAllCaps="true"
            android:textColor="@color/colorLightGrey" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorDarkForeground"
            android:orientation="vertical"
            android:padding="4dp">

            <android.support.design.widget.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:textColor="@color/colorWhite"
                android:textColorHint="@color/colorGrey">

                <EditText
                    android:id="@+id/alumni_one_et_address_province"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Provinsi*"
                    android:inputType="textCapWords"
                    android:maxLength="24"
                    android:maxLines="1"
                    android:selectAllOnFocus="true"
                    android:textColor="@color/colorWhite"
                    app:autoSizeTextType="uniform" />

            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:orientation="vertical"
                android:textColor="@color/colorWhite"
                android:textColorHint="@color/colorGrey">

                <EditText
                    android:id="@+id/alumni_one_et_address_city"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Kota*"
                    android:inputType="textCapWords"
                    android:maxLength="20"
                    android:maxLines="1"
                    android:selectAllOnFocus="true"
                    android:textColor="@color/colorWhite"
                    app:autoSizeTextType="uniform" />

            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:orientation="vertical"
                android:textColor="@color/colorWhite"
                android:textColorHint="@color/colorGrey">

                <EditText
                    android:id="@+id/alumni_one_et_address_district"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Kecamatan*"
                    android:inputType="textCapWords"
                    android:maxLength="20"
                    android:maxLines="1"
                    android:selectAllOnFocus="true"
                    android:textColor="@color/colorWhite"
                    app:autoSizeTextType="uniform" />

            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:orientation="vertical"
                android:textColor="@color/colorWhite"
                android:textColorHint="@color/colorGrey">

                <EditText
                    android:id="@+id/alumni_one_et_address_village"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Kecamatan*"
                    android:inputType="textCapWords"
                    android:maxLength="20"
                    android:maxLines="1"
                    android:selectAllOnFocus="true"
                    android:textColor="@color/colorWhite"
                    app:autoSizeTextType="uniform" />

            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:orientation="vertical"
                android:textColor="@color/colorWhite"
                android:textColorHint="@color/colorGrey">

                <EditText
                    android:id="@+id/alumni_one_et_address_street"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Alamat rumah*"
                    android:inputType="textCapWords|textMultiLine"
                    android:maxLines="3"
                    android:selectAllOnFocus="true"
                    android:textColor="@color/colorWhite" />

            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:orientation="vertical"
                android:textColor="@color/colorWhite"
                android:textColorHint="@color/colorGrey">

                <EditText
                    android:id="@+id/alumni_one_et_address_postcode"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Kode Pos"
                    android:inputType="number"
                    android:maxLength="7"
                    android:maxLines="1"
                    android:selectAllOnFocus="true"
                    android:textColor="@color/colorWhite"
                    app:autoSizeTextType="uniform" />

            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:orientation="vertical"
                android:textColor="@color/colorWhite"
                android:textColorHint="@color/colorGrey">

                <EditText
                    android:id="@+id/alumni_one_et_address_phone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Nomor telp. rumah"
                    android:inputType="phone"
                    android:maxLength="13"
                    android:maxLines="1"
                    android:selectAllOnFocus="true"
                    android:textColor="@color/colorWhite"
                    app:autoSizeTextType="uniform" />

            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:orientation="vertical"
                android:textColor="@color/colorWhite"
                android:textColorHint="@color/colorGrey">

                <EditText
                    android:id="@+id/alumni_one_et_address_hp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Nomor HP*"
                    android:inputType="phone"
                    android:maxLength="15"
                    android:maxLines="1"
                    android:selectAllOnFocus="true"
                    android:textColor="@color/colorWhite"
                    app:autoSizeTextType="uniform" />

            </android.support.design.widget.TextInputLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end"
                android:text="Misal: 081234567890"
                android:textColor="@color/colorLightGrey"
                android:textSize="12sp" />

        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginTop="4dp"
            android:layout_marginBottom="4dp"
            android:padding="4dp"
            android:text="Sosial Media"
            android:textAllCaps="true"
            android:textColor="@color/colorLightGrey" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorDarkForeground"
            android:orientation="vertical"
            android:padding="4dp">

            <android.support.design.widget.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:textColor="@color/colorWhite"
                android:textColorHint="@color/colorGrey">

                <EditText
                    android:id="@+id/alumni_one_et_email"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Email*"
                    android:inputType="textEmailAddress"
                    android:lines="1"
                    android:maxLength="32"
                    android:selectAllOnFocus="true"
                    android:textColor="@color/colorWhite"
                    app:autoSizeTextType="uniform" />

            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:orientation="vertical"
                android:textColor="@color/colorWhite"
                android:textColorHint="@color/colorGrey">

                <EditText
                    android:id="@+id/alumni_one_et_fb"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="URL Facebook (FB)"
                    android:inputType="textWebEmailAddress"
                    android:lines="1"
                    android:maxLength="32"
                    android:selectAllOnFocus="true"
                    android:textColor="@color/colorWhite"
                    app:autoSizeTextType="uniform" />

            </android.support.design.widget.TextInputLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end"
                android:text="Misal: https://facebook.com/raitoti/"
                android:textColor="@color/colorLightGrey"
                android:textSize="12sp" />

        </LinearLayout>

        <Button
            android:id="@+id/alumni_one_save_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:text="Simpan dan Lanjutkan"/>

    </LinearLayout>

</android.support.v4.widget.NestedScrollView>